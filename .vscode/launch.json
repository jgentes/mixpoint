  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug MixPoint",
      "request": "attach", // launch
      "type": "msedge",
      "url": "http://localhost:3000/*",
      "port": 9222, // remove if launch
      "preLaunchTask": "yarn dev",
      "webRoot": "${workspaceFolder}/src",
